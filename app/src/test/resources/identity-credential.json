{
  "format": "jwt_vc_json",
  "id": "UniversityDegree_JWT",
  "types": [
    "IdentityCredential"
  ],
  "cryptographic_binding_methods_supported": ["did"],
  "cryptographic_suites_supported": ["ES256K"],
  "display": [],
  "credentialSubject": {
    "last_name": {
      "mandatory": true,
      "value_type": "string",
      "display": [
        {"name": "Last Name", "locale": "en-US"},
        {"name": "姓", "locale": "ja-JP"},
      ]
    },
    "first_name": {
      "mandatory": true,
      "value_type": "string",
      "display": [
        {"name": "First Name", "locale": "en-US"},
        {"name": "名", "locale": "ja-JP"}
      ]
    },
    "previous_name": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "Previous Name", "locale": "en-US"},
        {"name": "旧姓", "locale": "ja-JP"}
      ]
    },
    "year": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "Birth Year", "locale": "en-US"},
        {"name": "生年月日(年)", "locale": "ja-JP"}
      ]
    },
    "month": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "Birth Month", "locale": "en-US"},
        {"name": "生年月日(月)", "locale": "ja-JP"}
      ]
    },
    "date": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "Birth Date", "locale": "en-US"},
        {"name": "生年月日(日)", "locale": "ja-JP"}
      ]
    },
    "prefecture": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "Prefecture", "locale": "en-US"},
        {"name": "都道府県", "locale": "ja-JP"}
      ]
    },
    "city": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "City", "locale": "en-US"},
        {"name": "市区町村", "locale": "ja-JP"}
      ]
    },
    "address": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "Address", "locale": "en-US"},
        {"name": "住所", "locale": "ja-JP"}
      ]
    },
    "sub_char_common_name": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "Sub Char Common Name", "locale": "en-US"},
        {"name": "姓名 代替文字使用位置", "locale": "ja-JP"}
      ]
    },
    "sub_char_previous_name": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "Sub Char Previous Name", "locale": "en-US"},
        {"name": "旧姓 代替文字使用位置", "locale": "ja-JP"}
      ]
    },
    "sub_char_address": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "Sub Char Address", "locale": "en-US"},
        {"name": "居住地(都道府県+市区町村+住所) 代替文字使用位置", "locale": "ja-JP"}
      ]
    },
    "gender": {
      "mandatory": false,
      "value_type": "string",
      "display": [
        {"name": "Gender", "locale": "en-US"},
        {"name": "性別", "locale": "ja-JP"}
      ]
    },
    "verified_at": {
      "mandatory": true,
      "value_type": "string",
      "display": [
        {"name": "Verified At", "locale": "en-US"},
        {"name": "確認日時", "locale": "ja-JP"}
      ]
    }
  },
  "order": ["Given Name", "Surname","GPA" ]
}
